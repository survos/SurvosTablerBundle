{# templates/components/ui/typed.html.twig v3.9 #}
{% set strings = strings | split('|')  %}
{% set id = id | default('typed')  %}
<span class="text-primary" id="{{ id  }}">{{ strings | first  }}</span>

{% capture_script  %}
<script>
document.addEventListener("DOMContentLoaded", function() {
	var typed = new Typed('#{{ id  }}', {
		strings: [{% for string in strings  %}'{{ string  }}'{% if not (loop.last)  %}, {% endif  %}{% endfor  %}],
		typeSpeed: 100,
		backSpeed: 50,
		backDelay: 1000,
		startDelay: 1000,
		loop: true,
		fade: true
	});
});
</script>
{% endcapture_script  %}

