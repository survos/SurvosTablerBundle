{% if app.session and app.session.started and app.session.flashbag.peekAll|length > 0 %}
    <div class="row">
        <div class="col-sm-12">
            {% for message in app.session.flashBag.all %}
                <li>
                    {{ message }}
                </li>
            {% endfor %}
        </div>
    </div>
{% endif %}
